<div class="listado">
    <h1 class="titulo">Lista de Productos</h1>
        <div>
        <table class="table">
        <thead class="cabecera">
            <tr>
                <th>Título</th>
                <th>Thumbnails</th>
                <th>Descripción</th>
                <th>Código</th>
                <th>Precio</th>
                <th>Estado</th>
                <th>Stock</th>
                <th>Categoría</th>
            </tr>
        </thead>
        
        <tbody class="filas">
            {{#each products}}
                <tr>
                    <td>{{this.title}}</td>
                    <td>{{this.thumbnails}}</td>
                    <td>{{this.description}}</td>
                    <td>{{this.code}}</td>
                    <td>{{this.price}}</td>
                    <td>{{this.status}}</td>
                    <td>{{this.stock}}</td>
                    <td>{{this.category}}</td>
                </tr>
            {{/each}}
        </tbody>
        </table>
        </div>
</div>